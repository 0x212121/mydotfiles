!- Color Scheme
!black
*color0: rgb:20/20/20
*color8: rgb:75/77/73
!red
*color1: rgb:cc/00/00
*color9: rgb:cc/29/29
!green
*color2: rgb:2e/8b/57
*color10: rgb:22/8b/22
!brown/yellow
*color3: rgb:c4/a0/00
*color11: rgb:fc/e9/4f
!blue
*color4: rgb:34/65/a4
*color12: rgb:72/9f/cf
!magenta
*color5: rgb:75/50/7b
*color13: rgb:ad/7f/a8
!cyan
*color6: rgb:06/98/9a
*color14: rgb:34/e2/e2
!white
*color7: rgb:d3/d7/cf
*color15: rgb:ee/ee/ec

!- Font Render
Xft.dpi:96
Xft.antialiasing:true
Xft.rgba:rgb
Xft.hinting:full
Xft.lcdfilter:lcddefault

URxvt.colorBD:              #ffffff
URxvt.colorIT:              #a761c2
URxvt.colorUL:              #6d9cbe
URxvt.underlineColor:       #333333
URxvt.highlightColor:       #00ffcc
URxvt.highlightTextColor:   #151515

URxvt.scrollstyle:	plain
URxvt.scrollBar:	false
URxvt.scrollTtyOutput:	false
URxvt.scrollWithBuffer:	true
URxvt.scrollTtyKeypress:true

!- Cursor settings
URxvt.cursorColor:          #00ffaa
URxvt.cursorColor2:         #00ffaa
URxvt.cursorBlink:          true
URxvt.cursorUnderline:      true

URxvt.saveLines: 	10000
URxvt.geometry: 	75x20
!URxvt.fading: 		30	!can cause breaking image on neofetch
URxvt.letterSpace: 0

URxvt.underlineURLs:	true
!\URxvt.font: xft:Droid\ Sans\ Mono\ Dotted\ for\ Powerline:size=9
URxvt.font: xft:Inconsolata\ for\ Powerline:size=9:antialias=true:hinting=true
URxvt.imFont: xft:Inconsolata\ for\ Powerline:size=9:antialias=true:hinting=true 
URxvt.boldFont: xft:Inconsolata\ for\ Powerline:bold:size=9:antialias=true:hinting=true
URxvt.italicFont: xft:Inconsolata\ for\ Powerline:italic:size=9:antialias=true:hinting=true
URxvt.boldItalicFont: xft:Inconsolata\ for\ Powerline:bold:italic:size=9:antialias=true:hinting=true

URxvt.intensityStyles:  true

URxvt.transparent:	false
!URxvt.shading:		0

!URxvt.iconFile: ~/Pictures/1212.jpg

URxvt*background: 	#303030
!urxvt.depth: 		24
URxvt.foreground: 	grey80
!URxvt.tintColor: 	#262626
URxvt.borderLess: 	false
URxvt.borderColor: 	#fff
URxvt.externalBorder: 	0
URxvt.internalBorder: 	1
URxvt.imLocale: pl_PL.ISO8859-2
URxvt.termName: rxvt-256color

!- Perl Extension
URxvt.perl-ext-common:default,matcher,url-select,clipboard
URxvt.url-launcher: chromium-browser
URxvt.matcher.button:1
 
!copy-paste
URxvt.iso14755: false
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.clipboard.copycmd:  xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard

!-Urgent Terminal for Notification
URxvt*urgentOnBell: false
URxvt*visualBell:   false

URxvt.skipBuiltGlyphs: true
